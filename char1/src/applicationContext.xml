<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="company" class="com.dx.po.Company">
        <property name="id" value="1"/>
        <property name="cname" value="阿里"/>
        <property name="depts" value="fds"/>
        <property name="address" value="深圳"/>
    </bean>
    <bean id="department" class="com.dx.po.Department">
        <property name="name" value="管理"/>
        <property name="cid" value="1"/>
        <property name="did" value="1"/>
        <property name="emps" value="sdf"/>
    </bean>
    <bean id="employee" class="com.dx.po.Employee">
        <property name="did" value="1"/>
        <property name="name" value="张收纳"/>
        <property name="age" value="13"/>
        <property name="eid" value="1"/>
        <property name="salary" value="10000"/>
        <property name="sex" value="男"/>
    </bean>
</beans>